{
  "moduleFileExtensions": [
    "js",
    "json",
    "ts"
  ],
  "verbose": true,
  "silent": false,
  "rootDir": "..",
  "testEnvironment": "node",
  "testRegex": "(.*).e2e-spec.ts$",
  "testTimeout": 10000,
  "maxWorkers": 1,
  "modulePaths": [
    "./"
  ],
  "transform": {
    "^.+\\.(t|j)s$": "ts-jest"
  },
  "moduleDirectories": [
    "node_modules"
  ],
  "collectCoverage": false,
  "coverageProvider": "v8",
  "collectCoverageFrom": [
    "src/**/*.ts",
    "!**/node_modules/**",
    "!**/dist/**"
  ]
}